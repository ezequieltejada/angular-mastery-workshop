{
  "name": "exercise-backend-communication",
  "version": "0.0.0",
  "scripts": {
    "start": "npm-run-all --parallel serve serve:api",
    "serve": "ng serve -o",
    "serve:api": "json-server --watch db.json --delay 250 --port 4300 --routes db-routes.json",
    "build": "ng build --configuration production",
    "test": "npm run format:test && ng lint && ng test --watch false",
    "watch": "ng test --reporters spec",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:write": "prettier projects/**/*.{js,ts,json,html,scss,md} --write",
    "format:test": "prettier projects/**/*.{js,ts,json,html,scss,md} --list-different",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer ./dist/customer-admin-app/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.6",
    "@angular/cdk": "^12.2.6",
    "@angular/common": "~12.2.6",
    "@angular/compiler": "~12.2.6",
    "@angular/core": "~12.2.6",
    "@angular/forms": "~12.2.6",
    "@angular/material": "^12.2.6",
    "@angular/platform-browser": "~12.2.6",
    "@angular/platform-browser-dynamic": "~12.2.6",
    "@angular/router": "~12.2.6",
    "@types/jest": "^24.0.19",
    "rxjs": "~6.6.2",
    "tslib": "^2.0.0",
    "uuid": "^3.3.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.6",
    "@angular/cli": "~12.2.6",
    "@angular/compiler-cli": "~12.2.6",
    "@angular/language-service": "~12.2.6",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "json-server": "^0.15.1",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-spec-reporter": "0.0.32",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
