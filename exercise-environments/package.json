{
  "name": "exercise-finished",
  "version": "0.0.0",
  "scripts": {
    "start": "npm-run-all --parallel serve serve:api",
    "serve": "ng serve -o",
    "serve:api": "json-server --watch db.json --delay 250 --port 4300 --routes db-routes.json",
    "serve:api:test": "json-server --watch db-test.json --delay 250 --port 4400 --routes db-routes.json",
    "serve:api:prod": "json-server --watch db-prod.json --delay 250 --port 4500 --routes db-routes.json",
    "build": "ng build --prod",
    "test": "npm run format:test && ng lint && ng test --watch false --code-coverage",
    "watch": "ng test --reporters spec",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:write": "prettier projects/**/*.{js,ts,json,html,scss,md} --write",
    "format:test": "prettier projects/**/*.{js,ts,json,html,scss,md} --list-different",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/customer-admin-app/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.8",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~10.0.8",
    "@angular/compiler": "~10.0.8",
    "@angular/core": "~10.0.8",
    "@angular/forms": "~10.0.8",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~10.0.8",
    "@angular/platform-browser-dynamic": "~10.0.8",
    "@angular/router": "~10.0.8",
    "@types/jest": "^24.0.19",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.6.2",
    "tslib": "^2.0.0",
    "uuid": "^3.3.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.5",
    "@angular/cli": "~10.0.5",
    "@angular/compiler-cli": "~10.0.8",
    "@angular/language-service": "~10.0.8",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "json-server": "^0.15.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-spec-reporter": "0.0.32",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
